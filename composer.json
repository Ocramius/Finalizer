{
    "name":        "ocramius/finalizer",
    "description": "A simple utility library that checks for PHP classes that should or shouldn't be marked as final",
    "type":        "library",
    "license":     "MIT",
    "homepage":    "https://github.com/Ocramius/Finalizer",
    "keywords":    [
        "final",
        "syntax",
        "code quality"
    ],
    "authors": [
        {
            "name":     "Marco Pivetta",
            "email":    "ocramius@gmail.com",
            "homepage": "http://ocramius.github.io/"
        }
    ],
    "autoload": {
        "psr-0": {
            "Finalizer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "FinalizerTest\\":      "test",
            "FinalizerTestAsset\\": "test"
        }
    },
    "require": {
        "php":             "~5.6|~7.0",
        "symfony/console": "~2.6|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "bin": [
        "finalizer"
    ]
}
